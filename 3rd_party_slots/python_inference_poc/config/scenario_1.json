{
    "seed": 42,
    "gen_seed": 11,
    "metta-filename": "/opt/das/3rd_party_slots/python_inference_poc/output.metta",
    "sentence-node-count": 100000,
    "word-count": 3,
    "word-length": 3,
    "alphabet-range": "0-4",
    "letter-predicates": [
        {
            "token": "feature_is_{letter_percent}_letter_{letter}",
            "letter": "a",
            "letter_percent": 0.5,
            "percentage": 1
        },
        {
            "token": "feature_is_{letter_percent}_letter_{letter}",
            "letter": "b",
            "letter_percent": 0.5,
            "percentage": 1
        },
        {
            "token": "feature_is_{letter_percent}_letter_{letter}",
            "letter": "c",
            "letter_percent": 0.5,
            "percentage": 1
        }
    ],
    "wildcard-predicates": [{
        "token": "feature_contains_*",
        "wildcards": ["*ac", "cb*"],
        "percentage": 0.1
    }],
    "word-predicate": {
        "token": "feature_contains_*",
        "word-count": 1,
        "alphabet-range": "0-4",
        "percentage": 0.1
    },
    "start-end-predicates": [{
        "token": "starts_{start_letter}_end_{end_letter}",
        "start_letter": "c",
        "end_letter": "d",
        "percentage": 0.1
    }],
    "low-diversity-predicates": [{
        "token": "feature_low_diversity_of_{letters}",
        "letters": ["a", "b", "c", "d", "e"],
        "percentage": 1
    }],
    "bias":{
        "predicates": ["feature_is_0.5_letter_c", "starts_c_end_d", "feature_low_diversity_of_a_b_c_d_e"],
        "strength": 0.3,
        "filename": "/opt/das/3rd_party_slots/python_inference_poc/biased_predicates.metta",
        "operator": "and"
    }
}
