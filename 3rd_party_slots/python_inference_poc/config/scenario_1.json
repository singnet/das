{
    "seed": 42,
    "metta-filename": "output.metta",
    "sentence-node-count": 3,
    "word-count": 3,
    "word-length": 3,
    "alphabet-range": "0-3",
    "letter-predicate": {
        "token": "feature_a",
        "letter": "a",
        "letter_percent": 0.5,
        "randomness": 0.8
    },
    "wildcard-predicate": {
        "token": "feature_b",
        "wildcards": ["*ac", "cb*"],
        "randomness": 0.99
    },
    "start-end-predicate": {
        "token": "feature_c",
        "start_letter": "c",
        "end_letter": "d",
        "randomness": 0.99
    },
    "low-diversity-predicate": {
        "token": "feature_d",
        "letters": ["a", "b", "c", "d", "e"],
        "randomness": 0.8
    },
    "bias":{
        "predicates": ["letter-predicate", "low-diversity-predicate"],
        "strength": 0.3,
        "filename": "biased_predicates.metta"
    }
}