{
    "seed": 42,
    "metta-filename": "/opt/das/3rd_party_slots/python_inference_poc/output.metta",
    "sentence-node-count": 10000,
    "word-count": 3,
    "word-length": 3,
    "alphabet-range": "0-4",
    "letter-predicate": {
        "token": "feature_a",
        "letter": "a",
        "letter_percent": 0.5,
        "percentage": 1
    },
    "wildcard-predicate": {
        "token": "feature_b",
        "wildcards": ["*ac", "cb*"],
        "percentage": 0.1
    },
    "start-end-predicate": {
        "token": "feature_c",
        "start_letter": "c",
        "end_letter": "d",
        "percentage": 0.1
    },
    "low-diversity-predicate": {
        "token": "feature_d",
        "letters": ["a", "b", "c", "d", "e"],
        "percentage": 1
    },
    "bias":{
        "predicates": ["letter-predicate", "low-diversity-predicate"],
        "strength": 0.3,
        "filename": "/opt/das/3rd_party_slots/python_inference_poc/biased_predicates.metta",
        "operator": "and"
    }
}